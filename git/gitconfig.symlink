# Local/private config goes in the include
[include]
        path = ~/.gitconfig.local
[alias]
        co = checkout
        ci = commit
        st = status
        br = branch
        hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
        type = cat-file -t
        dump = cat-file -p
[core]
        autocrlf = input
        safecrlf = true
        excludesfile = ~/.gitignore
        editor = vim
[diff]
	compactionHeuristic = true
[help]
        autocorrect = 1
